  <rdf:Description rdf:about="http://sebk.me/MOOC.owl#coursera_course129">
    <schema:timeRequired>6-8 hours/week</schema:timeRequired>
    <schema:video>https://www.youtube.com/watch?v=SCGlbr1t628</schema:video>
