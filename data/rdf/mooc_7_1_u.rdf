  <rdf:Description rdf:about="http://sebk.me/MOOC.owl#coursera_course114">
    <schema:video>https://www.youtube.com/watch?v=PJnd28w1oX8</schema:video>
    <isTaughtBy>http://sebk.me/MOOC.owl#coursera_instructor920057</isTaughtBy>
