    <schema:name>A Beginner's Guide to Irrational Behavior</schema:name>
    <hasCategory>http://sebk.me/MOOC.owl#coursera_category6</hasCategory>
    <schema:timeRequired>7-11 hours/week</schema:timeRequired>
    <hasCategory>http://sebk.me/MOOC.owl#coursera_category2</hasCategory>
