  <rdf:Description rdf:about="http://sebk.me/MOOC.owl#coursera_course1051">
    <rdf:type rdf:resource="http://sebk.me/MOOC.owl#Course"/>
    <isTaughtBy>http://sebk.me/MOOC.owl#coursera_instructor3096030</isTaughtBy>
