  <rdf:Description rdf:about="http://sebk.me/MOOC.owl#coursera_course1399">
    <Recommended_Background>&lt;p&gt;No particular background is required. All are welcome.&lt;/p&gt;</Recommended_Background>
    <hasCategory>http://sebk.me/MOOC.owl#coursera_category8</hasCategory>
