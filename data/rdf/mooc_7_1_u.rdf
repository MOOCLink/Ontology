    <schema:name>Managing Fashion and Luxury Companies</schema:name>
    <isTaughtBy>http://sebk.me/MOOC.owl#coursera_instructor5179334</isTaughtBy>
    <Course_Format>&lt;p&gt;The class will consist of lecture videos, which are about 10 minutes in length, plus reading and assignments.&lt;br&gt;&lt;/p&gt;</Course_Format>
    <rdf:type rdf:resource="http://sebk.me/MOOC.owl#Course"/>
    <Course_ID>1132</Course_ID>
