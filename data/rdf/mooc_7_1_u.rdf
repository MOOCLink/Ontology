    <schema:name>The Clinical Psychology of Children and Young People</schema:name>
    <hasCategory>http://sebk.me/MOOC.owl#coursera_category10</hasCategory>
    <hasCategory>http://sebk.me/MOOC.owl#coursera_category26</hasCategory>
