  <rdf:Description rdf:about="http://sebk.me/MOOC.owl#coursera_course329">
    <schema:video>https://www.youtube.com/watch?v=0nmxFpNBFIY</schema:video>
    <isTaughtBy>http://sebk.me/MOOC.owl#coursera_instructor2068199</isTaughtBy>
